#include <iostream>
using namespace std;
int Tusk = 4;
int main() {
   if (Tusk == 1){
       int a, summ;
       cin >> a;
       for (int i = 1; i <= a; i++){
            summ = summ + i;
       }
       cout << summ;
   }
   if (Tusk == 2){
       float lvl;
       cout << "Уровень" << " Год" << endl;
       for (int i = 1; i <= 25; i++){
           lvl += 1.5;
           cout << lvl << "\t" << i << endl;
       }
   }
   if (Tusk == 3){
       cout << "Время" << " Каллории" << endl;
       for (int time = 5; time <= 30; time += 5){
           cout << time << "\t  " << time * 3.6 << endl;
       }
   }
   if (Tusk == 4){
       cout << "Напишите скорость транспорта (миль/час) и сколько часов ехало" << endl;
       int time; 
       float speed;
       cin >> speed >> time;
       if (speed <= -1 || time < 1){
           cout << "Вы ввели не правильно значение";
           return 0;
       }
       cout << "Час" << " Пробег" << endl;
       for(int i = 1; i <= time; i++){
           cout << i << "\t  " << speed * i << endl; 
       }
   }
   if (Tusk == 5){
       
   }
    return 0;
}
