#include <iostream>
#include <cmath>
#include <ctime>
using namespace std;
int Tusk = 8;
int main() {
   if (Tusk == 1){
       int a, summ;
       cin >> a;
       for (int i = 1; i <= a; i++){
            summ = summ + i;
       }
       cout << summ;
   }
   if (Tusk == 2){
       float lvl;
       cout << "Уровень" << " Год" << endl;
       for (int i = 1; i <= 25; i++){
           lvl += 1.5;
           cout << lvl << "\t" << i << endl;
       }
   }
   if (Tusk == 3){
       cout << "Время" << " Каллории" << endl;
       for (int time = 5; time <= 30; time += 5){
           cout << time << "\t  " << time * 3.6 << endl;
       }
   }
   if (Tusk == 4){
       cout << "Напишите скорость транспорта (миль/час) и сколько часов он ехал" << endl;
       int time; 
       float speed;
       cin >> speed >> time;
       if (speed <= -1 || time < 1){
           cout << "Вы ввели не правильно значение";
           return 0;
       }
       cout << "Час" << " Пробег" << endl;
       for(int i = 1; i <= time; i++){
           cout << i << "\t  " << speed * i << endl; 
       }
   }
   if (Tusk == 5){
       int day;
       double PayDay,summ;
       cout << "Введите кол-во рабочих дней" << endl;
       cin >> day;
       if (day < 1){
           cout << "Введено не правильное значение";
           return 0;
       }
       cout << "День" << " Зарплата" << endl;
       for (int i = 1; i <= day; i++){
            PayDay = (pow(2, i - 1)) / 100;
            summ += PayDay;
            cout << i << "\t" << PayDay << endl;
       }
       cout << "Общая сумма - $" << summ;
   }
   if (Tusk == 6){
       int floorZ, room, used, allRoom = 0, usedRoom = 0, voidRoom = 0;
       double procent;
       cout << "Сколько этажей в отеле?" << endl;
       cin >> floorZ;
       if (floorZ < 1){
           cout << "Сколько этажей в отеле?";
           return 0;
       }
       for (int i = 1; i <= floorZ; i++){
           if (i == 13){
               continue;
           }
           cout << "Сколько комнат на " << i << " этаже? Сколько из них занято?" << endl;
           cin >> room >> used;
           allRoom += room;
           usedRoom += used;
           voidRoom += (room - used);
       }
       procent = (usedRoom / allRoom) * 100;
       cout << "Общее число комнат - " << allRoom << endl;
       cout << "Занятые комнаты - " << usedRoom << endl;
       cout << "Свободные комнаты - " << voidRoom << endl;
       cout << "Процент занятых - " << procent << "%";
   }
   if (Tusk == 7){
        int personaNumber;
        double valueMoney, tax, taxFed, fica;
        double totalValue = 0, totalTax = 0, totalTaxFed = 0, totalFica = 0, whiteMoney = 0;
        while (true){
            cout << "Введите номер сотрудника (Введите 0 для выхода)" << endl;
            cin >> personaNumber;
        
            if (personaNumber == 0){
                break;
            }
        
            cout << "Введите валовую зарпалату" << endl;
            cin >> valueMoney;
        
            cout << "Введите штатный налог" << endl;
            cin >> tax;
        
            cout << "Введите федеральный налог" << endl;
            cin >> taxFed;
        
            cout << "Введите удержание FICA" << endl;
            cin >> fica;
            if ((fica || taxFed || tax) > valueMoney){
                cout << "Значение налогов/удержаний не может быть больше зарплаты" << endl;
                continue;
            }
            if (valueMoney < 0 || tax < 0 || taxFed < 0){
                cout << "начение налогов/удержаний не может быть равно 0" << endl;
                continue;
            }
        
            if (fica + tax + taxFed > valueMoney){
                cout << "Сумма налогов и удержаний не могут быть больше зарплаты" << endl;
                continue;
            }
            totalValue += valueMoney;
            totalTax += tax;
            totalTaxFed += taxFed;
            totalFica += fica;
            whiteMoney += (valueMoney - (tax + taxFed + fica));
           }
           cout << "Итог вaловой зарплаты - " << totalValue << endl;
           cout << "Итог штатного налога - " << totalTax << endl;
           cout << "Итог федерального налога - " << totalTaxFed << endl;
           cout << "Итог FICA - " << totalFica << endl;
           cout << "Итог чистой зарплаты - " << whiteMoney << endl;
   }
   if (Tusk == 8){
       srand(time(0));
       int randomNumber = (rand()) / 10000000, i, tryN = 0;
       cout << "Угадай число" << endl;
       while (true){
          cin >> i;
          tryN++;
          if (i == randomNumber){
              cout << "Угадал" << endl;
              cout << "Кол-во попыток - " << tryN;
          } else if (i > randomNumber){
              cout << "Too high, try again." << endl;;
          } else{
              cout << "Too low, try again." << endl;
          }
       }
   }
    return 0;
}
